{
  "extends": ["airbnb/base", "plugin:import/errors"],
  "rules": {
    "id-length": 0,
    "max-len": 0,
    "no-param-reassign": [2, { "props": false }],
    "consistent-return": 0,
    "arrow-parens": 0,
    "object-curly-newline": ["error", {
      "ObjectPattern": { "multiline": false }
    }],
    "implicit-arrow-linebreak": 0
  }
}
